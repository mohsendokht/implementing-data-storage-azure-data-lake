{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "HamidADF01"
		},
		"LS_AzureDataLakeStorage_hamidsag2test1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage_hamidsag2test1'"
		},
		"LS_AzureBlobStorage_hamidstorageaccount01_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secret-hamidstorageaccount01"
		},
		"LS_AzureDataLakeStorage_hamidsag2test1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://hamidsag2test1.dfs.core.windows.net/"
		},
		"LS_HamidKeyVault02_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://HamidKeyVault02.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "HamidADF01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorage_hamidsag2test1_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage_hamidsag2test1_accountKey')]"
					},
					"LS_AzureDataLakeStorage_hamidsag2test1_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_hamidsag2test1_properties_typeProperties_url')]"
					},
					"LS_HamidKeyVault02_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_HamidKeyVault02_properties_typeProperties_baseUrl')]"
					},
					"LS_AzureBlobStorage_hamidstorageaccount01_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_AzureBlobStorage_hamidstorageaccount01_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}